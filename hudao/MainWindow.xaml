<Window x:Class="hudao.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" WindowStartupLocation="CenterScreen"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" x:Name="MainWindowContainer" BorderBrush="{DynamicResource LineBrush}" BorderThickness="1"
        Title="MainWindow" Height="768" Width="1024">
    <AdornerDecorator>
        <Grid>
            <ContentControl x:Name="ViewContainer">
            </ContentControl>
            <StackPanel Orientation="Vertical" x:Name="MessageContainer">
            </StackPanel>
        </Grid>
    </AdornerDecorator>
</Window>
