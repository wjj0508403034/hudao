<Controls1:BasicGroupListPanelControl x:Class="hudao.Views.Inventory.SidePanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:Controls1="clr-namespace:hudao.Core.Controls"
                                      xmlns:GroupList="clr-namespace:Controllers.Controls.GroupList;assembly=Controllers"
                                      xmlns:Common="clr-namespace:hudao.Views.Common" mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <StackPanel Orientation="Vertical">
        <Common:SideHeader></Common:SideHeader>
        <GroupList:GroupListBox x:Name="TestListBox"   GroupMemberPath="CreateTime" ItemTemplate="{DynamicResource InventorySideBarItem}"  >
        </GroupList:GroupListBox>
    </StackPanel>
</Controls1:BasicGroupListPanelControl>
